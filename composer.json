{
    "name": "innmind/socket",
    "type": "library",
    "description": "Wrapper to PHP socket functions",
    "keywords": ["socket"],
    "homepage": "http://github.com/Innmind/Socket",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Socket/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/stream": "dev-capabilities",
        "innmind/immutable": "~4.0",
        "innmind/ip": "~3.0",
        "innmind/url": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Socket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Socket\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "symfony/process": "^5.0",
        "vimeo/psalm": "~4.1",
        "innmind/coding-standard": "~2.0"
    }
}
